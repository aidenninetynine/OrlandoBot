{
  "name": "@solid-primitives/map",
  "version": "0.4.3",
  "description": "The Map & WeakMap data structures as a reactive signals.",
  "author": "Damian Tarnawski @thetarnav <gthetarnav@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/solidjs-community/solid-primitives/tree/main/packages/utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "map",
    "stage": 2,
    "list": [
      "ReactiveMap",
      "ReactiveWeakMap"
    ],
    "category": "Utilities"
  },
  "keywords": [
    "map",
    "weakmap",
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "dependencies": {
    "@solid-primitives/trigger": "^1.0.3"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "page": "vite build dev",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  }
}